<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.collections.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>

<?import javafx.scene.text.Font?>
<AnchorPane minWidth="315.0" prefHeight="307.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" >
    <children>
        <VBox id="VBox" layoutX="14.0" layoutY="17.0" spacing="5.0">
            <children>
                <Label fx:id="instructions"  prefWidth="200.0" text="Paste the locations of your java files. The before field should be the location of your file before the change. The after field is the location of your file with the change." wrapText="true" textAlignment="CENTER" >
                    <font>
                        <Font size="10.0" />
                    </font>
                </Label>
                <Button fx:id="leftButton" mnemonicParsing="false" prefWidth="-1.0" text="Left" VBox.vgrow="SOMETIMES" />
                <Button fx:id="rightButton" mnemonicParsing="false" prefWidth="-1.0" text="Right" VBox.vgrow="SOMETIMES" />
                <Button fx:id="myButton" mnemonicParsing="false" prefWidth="-1.0" text="Process Change" VBox.vgrow="SOMETIMES" />
            </children>
        </VBox>
        <TextArea fx:id="outputTextArea" layoutX="13.0" layoutY="194.0" prefHeight="400.0" prefWidth="395.0" wrapText="true" />
        <Separator layoutX="14.0" layoutY="182.0" prefWidth="395.0" />
    </children>
</AnchorPane>
